<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp5.0</TargetFramework>
    <Version>1.0.0</Version>
    <Company>FB_BRAND</Company>
    <Authors>FB_BRAND</Authors>
    <Product>FB_BRAND </Product>
    <Description />
    <Copyright>FB_COPYRIGHT</Copyright>
    <AspNetCoreHostingModel>InProcess</AspNetCoreHostingModel>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <NoWarn>$(NoWarn);1591</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper" Version="11.0.1" />
    <PackageReference Include="AutoMapper.Extensions.Microsoft.DependencyInjection" Version="11.0.0" />
    <PackageReference Include="FewBox.Core.Persistence" Version="1.0.44" />
    <PackageReference Include="FewBox.Core.Utility" Version="1.0.38" />
    <PackageReference Include="FewBox.SDK" Version="0.0.18" />
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="6.0.4" />
    <PackageReference Include="Microsoft.AspNetCore.SignalR.Core" Version="1.1.0" />
    <PackageReference Include="NSwag.AspNetCore" Version="13.15.10" />
    <PackageReference Include="Sentry.AspNetCore" Version="3.17.0" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Versioning" Version="5.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Versioning.ApiExplorer" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\FewBox.Template.Service.Model\FewBox.Template.Service.Model.csproj" />
    <ProjectReference Include="..\FewBox.Template.Service.Domain\FewBox.Template.Service.Domain.csproj" />
    <ProjectReference Include="..\FewBox.Template.Service.Repository\FewBox.Template.Service.Repository.csproj" />
    <ProjectReference Include="..\FewBox.Template.Service.Boot\FewBox.Template.Service.Boot.csproj"
      OutputItemType="Analyzer"
      ReferenceOutputAssembly="false" />
  </ItemGroup>

  <ItemGroup>
    <CompilerVisibleProperty Include="DebugSourceGenerators" />
    <CompilerVisibleProperty Include="DebugThisAssemblyMetadata" />
    <CompilerVisibleProperty Include="LowCode_IsDebug" />
    <!--<CompilerVisibleItemMetadata Include="AdditionalFiles" MetadataName="LowCode_Debug" />-->
    <AdditionalFiles Include="Entities/*.json" />
    <AdditionalFiles Include="Services/*.json" />
  </ItemGroup>

</Project>